// vim: ft=c
/* initial setup */
frames 30
basename frame
light 0.0 0.0 -1.0
constants global 20 20 20 1.0 1.0 1.0 50 50 50 1.0 1.0 1.0 100 100 100 1.0 1.0 1.0 3
resize 1400 900

/* script */
push
scale 0.9 0.9 0.9

/* body */
draw-mode lines
render-mode wireframe
shade-mode flat
push
move 150 250 0
rotate y 30
rotate x 20
box -75 100 40 150 200 80
/* head */
push
move 0 151 0
sphere 0 0 0 50
/* left arm */
pop
push
move 75 100 0
rotate z -30
rotate x 180
box 0 0 20 40 180 40
/* ball */
constants ball 120 20 20 1.0 1.0 1.0 50 50 50 1.0 1.0 1.0 100 100 100 1.0 1.0 1.0 3
sphere 20 -190 0 20 0.08
constants global 20 20 20 1.0 1.0 1.0 50 50 50 1.0 1.0 1.0 100 100 100 1.0 1.0 1.0 3
/* right arm */
pop
push
move -75 100 0
rotate x -30
box 0 0 0 -40 180 40
/* right leg */
pop
push
move -70 -100 0
box 0 0 0 40 140 40
/* left leg */
pop
push
move 30 -100 0
box 0 0 0 40 140 40
/* crown */
pop
push
constants crown 20 20 120 1.0 1.0 1.0 50 50 50 1.0 1.0 1.0 100 100 100 1.0 1.0 1.0 3
torus 0 200 0 20 60 0.05
constants global 20 20 20 1.0 1.0 1.0 50 50 50 1.0 1.0 1.0 100 100 100 1.0 1.0 1.0 3
